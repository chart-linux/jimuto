<p id="notice"><%= notice %></p>

<h1>Request Solutions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @request_solutions.each do |request_solution| %>
      <tr>
        <td><%= link_to 'Show', request_solution %></td>
        <td><%= link_to 'Edit', edit_request_solution_path(request_solution) %></td>
        <td><%= link_to 'Destroy', request_solution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request Solution', new_request_solution_path %>
